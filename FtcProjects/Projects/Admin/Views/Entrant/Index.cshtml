<br />
<ul>
    <li><a href="#" id="Dublicates">Удаление дублирующих записей об абитуриентах</a></li>
</ul>
<br />

<div class="form-group">
    <div class="col-md-8">
        <div id="result" />
    </div>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        $('#Dublicates').click(function (e) {
            e.preventDefault();
            $.ajax({
                type: "POST",
                url: '@Url.Action("Dublicates", "Entrant")',
                success: function (msg) {
                    $('#result').empty().html(msg);
                }
            });
        });

    });

</script>