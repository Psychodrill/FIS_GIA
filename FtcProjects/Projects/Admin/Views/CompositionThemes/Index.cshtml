

<form enctype="multipart/form-data" asp-action="AddFile" asp-controller="CompositionThemes" data-ajax="true" data-ajax-success="onSuccess" method="post" >
    <div class="mb-3">
        <input class="form-control" type="file" id="upload" name="uploadFile" multiple>
        <button type="submit" id="load">Загрузить</button>
    </div>
</form>



<div class="form-group">
    <div class="col-md-8">
        <div id="result" />
    </div>
</div>


<script type="text/javascript">
    function onSuccess(data) {
        $('#result').empty().html(data);
        console.log(data);
    }
</script>