
<br />
<ul>
    <li><a href="#" id="EditCompGrps" name="EditCompGrps">Редактирование Конкурсных групп </a></li>
</ul>
<br />

<div class="form-group">
    <div class="col-md-8">
        <div id="result" />
    </div>
</div>





<script type="text/javascript">

    $(document).ready(function () {
        $('#EditCompGrps').click(function (e) {
            e.preventDefault();
            $.ajax({
                type: "POST",
                url: '@Url.Action("EditCompetitiveGroup", "CompetitiveGroup")',
                success: function (msg) {
                $('#result').empty().html(msg);
                }
            });
        });


    });

</script>