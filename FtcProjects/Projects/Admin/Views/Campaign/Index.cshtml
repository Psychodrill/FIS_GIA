
<br />
<ul>
    <li><a href="#" id="EditCampaign" name="EditCampaign">Редактирование Приемных кампаний </a></li>
</ul>
<br />

    <div class="form-group">
        <div class="col-md-8">
            <div id="result" />
        </div>
    </div>





<script type="text/javascript">

    $(document).ready(function () {
       $('#EditCampaign').click(function (e) {
            e.preventDefault();
            $.ajax({
                type: "POST",
                url: '@Url.Action("EditCampaign", "Campaign")',
                success: function (msg) {
                $('#result').empty().html(msg);
                }
            });
        });


    });

</script>